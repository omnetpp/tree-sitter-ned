{
  "name": "omnetpp-ned",
  "version": "0.0.4",
  "description": "Tree-sitter parser for the OMNeT++ NED language",
  "main": "grammar.js",
  "types": "bindings/node",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-gyp-build": "^4.8.0",
    "node-addon-api": "^7.1.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.22.6"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "tree-sitter": [
    {
      "scope": "source.ned",
      "injection-regex": "^ned$",
      "file-types": [
        "ned"
      ]
    }
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omnetpp/tree-sitter-ned.git"
  },
  "keywords": [
    "omnetpp",
    "ned"
  ],
  "bugs": {
    "url": "https://github.com/omnetpp/tree-sitter-ned/issues"
  },
  "homepage": "https://github.com/omnetpp/tree-sitter-ned#readme"
}

{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "omnetpp-ned",
      "camelcase": "OmnetppNed",
      "title": "omnetpp-ned",
      "scope": "source.ned",
      "file-types": [
        "ned"
      ],
      "injection-regex": "^ned$",
      "class-name": "TreeSitterNed"
    }
  ],
  "metadata": {
    "version": "0.0.5",
    "license": "MIT",
    "description": "Tree-sitter parser for the OMNeT++ NED language",
    "authors": [
      {
        "name": "Gyorgy Szaszko",
        "email": "adam.szaszko@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/omnetpp/tree-sitter-ned"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
